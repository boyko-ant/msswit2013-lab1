@model msswit2013_lab1.core.Table.Entry.Category

@{
	ViewBag.Title = "Category entry";
	Layout = "~/Views/Admin/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm())
{
	@Html.HiddenFor(m => Model.RowKey)
	<table>
		<tr>
			<th>@Html.LabelFor(m => Model.Title)</th>
			<td>@Html.EditorFor(m => Model.Title)</td>
		</tr>
	</table>
	<input type="submit" value="submit"/>
}